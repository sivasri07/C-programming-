#include <stdio.h>
#include <math.h>
int main() {
    int num, originalNum, remainder, result = 0;
    scanf("%d", &num);
    if (num < 100 || num > 999) {
        printf("No\n");
        return 0;
    }

    originalNum = num;
    while (originalNum != 0) {
        remainder = originalNum % 10;
        result += remainder * remainder * remainder;
        originalNum /= 10;
    }
    if (result == num) {
        printf("Yes\n");
    } else {
        printf("No\n");
    }

    return 0;
}
