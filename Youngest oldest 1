#include <stdio.h>
int main() {
    int n, i;
    scanf("%d", &n);

    if (n <= 0) {
        printf("Invalid Input");
        return 0;
    }

    int age[n];
    for (i = 0; i < n; i++) {
        scanf("%d", &age[i]);
        if (age[i] < 0) {
            printf("Invalid Input");
            return 0;
        }
    }

    int youngest = age[0];
    int oldest = age[0];

    for (i = 1; i < n; i++) {
        if (age[i] < youngest) {
            youngest = age[i];
        }
        if (age[i] > oldest) {
            oldest = age[i];
        }
    }

    printf("Youngest=%d\n", youngest);
    printf("Oldest=%d\n", oldest);

    return 0;
}

